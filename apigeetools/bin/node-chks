#/bin/bash

curl -w '[%{http_code}]\n' http://localhost:8080/self/up
curl -w '[%{http_code}]\n' http://localhost:8081/self/up
curl -w '[%{http_code}]\n' http://localhost:8082/self/up
curl -w '[%{http_code}]\n' http://localhost:8081/runtime
curl -w '[%{http_code}]\n' http://localhost:8082/runtime/o/$ORGANIZATION/e/$ENV/apis/mongo
curl -w '[%{http_code}]\n' http://localhost:8081/runtime/organizations/$ORGANIZATION/environments/$ENV/apis/mongo -w ' %{http_code} '
curl -w '[%{http_code}]\n' http://localhost:8081/runtime/organizations
curl -w '[%{http_code}]\n' http://localhost:8082/organizations/$ORGANIZATION/environment/$ENV/apis/mongo
curl -w '[%{http_code}]\n' http://localhost:8082/organizations/$ORGANIZATION/environments/$ENV/apis/mongo -w '[%{http_code}]\n'
curl -w '[%{http_code}]\n' http://localhost:8082/organizations/$ORGANIZATION/environments/$(uri_escape "$ENV")/apis
curl -w '[%{http_code}]\n' http://localhost:8082/organizations/$ORGANIZATION/apis
curl -w '[%{http_code}]\n' http://localhost:8082/organizations/$ORGANIZATION/apis/mongo
curl -w '[%{http_code}]\n' http://localhost:8081/classification/tree
curl -w '[%{http_code}]\n' http://localhost:8081/servers/self/up
curl -w '[%{http_code}]\n' http://localhost:8081/servers/self
curl -w '[%{http_code}]\n' http://localhost:8082/servers/self
curl -w '[%{http_code}]\n' http://localhost:8082/servers/self/up
curl -w '[%{http_code}]\n' http://localhost:8081/classification/tree
curl -w '[%{http_code}]\n' http://localhost:8082/classification/tree
curl -w '[%{http_code}]\n' http://localhost:8082/runtime/organizations/$ORGANIZATION/environments/$ENV/apis
