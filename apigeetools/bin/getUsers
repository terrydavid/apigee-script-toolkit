#!/bin/bash

# getUsers
#
# lists the users in an organization
#
# usage:
#   getUsers host_alias

#set -e

USAGE=" # ouput is simple list"
TOOLSDIR="${APITOOLS_HOME}"
. "$TOOLSDIR/lib/functions"
parseCommandline "$@"
checkArgs 1

# URL="${APIGEE_HOST}/o/${ORGANIZATION}/users"
URL="${APIGEE_HOST}/users"

http_get "${URL}"

echo "${RESPONSE}"
