#!/bin/bash

# getTargetServer
#
# lists the virtual hosts defined for an environment
#
# usage:
#   getVirtualHosts environment

#set -e

USAGE="targetserver"
TOOLSDIR="${APITOOLS_HOME}"

. "$TOOLSDIR/lib/functions"

parseCommandline "$@"

URL="${APIGEE_HOST}/o/${ORGANIZATION}/e/${ENVIRONMENT}/targetservers/${ARGS[0]}"

http_delete "$URL"

echo "$RESPONSE"
