#!/bin/bash

# getSFRevisionLatest
#
# returns the revision number of the newest version of a sharedflow
#
# usage:
#   getSFRevisionLatest api

[[ $DEBUG -ge 4 ]] && set -x

USAGE="shared_flow_name"
TOOLSDIR="${APITOOLS_HOME}"
. "$TOOLSDIR/lib/functions"
parseCommandline "$@"
checkArgs 2

debug "getSFlowRevisions $@ | tail -1"

getApiRevisions $@ | tail -1
