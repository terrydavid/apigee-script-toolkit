#!/bin/bash

# getCache
#
# retrieves the specified cache
#
# usage:
#   getCache cache_name

#set -e

USAGE="cache_name # This only works from localhost"
TOOLSDIR="${APITOOLS_HOME}"
. "$TOOLSDIR/lib/functions"
parseCommandline "$@"
checkArgs 2

cache_name="${ARGS[0]}"

#URL="${APIGEE_HOST}/runtime/o/${ORGANIZATION}/e/${ENVIRONMENT}/caches/$(uri_escape "$cache_name")/entries?action=clear"
URL="${APIGEE_HOST}/o/${ORGANIZATION}/e/${ENVIRONMENT}/caches/"${cache_name}"/entries"

debug "Getting Cache Entries for $URL"

http_get "$URL"
